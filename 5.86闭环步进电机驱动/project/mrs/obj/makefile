86_Stepper_motor_drive.siz \


# 所有目标
all: 86_Stepper_motor_drive.elf secondary-outputs

# 工具调用
86_Stepper_motor_drive.elf: $(OBJS) $(USER_OBJS)
	@	@	riscv-none-embed-gcc -march=rv32imafc -mabi=ilp32f -msmall-data-limit=8 -mno-save-restore -O0 -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -pedantic -Wunused -Wuninitialized -Wall  -g -T "X:\300-软件技术\205-我写的MCU程序\CH32\5.86闭环步进电机驱动\libraries\sdk\Ld\Link.ld" -nostartfiles -Xlinker --gc-sections -L"../" -L"X:\300-软件技术\205-我写的MCU程序\CH32\5.86闭环步进电机驱动\libraries\zf_device" -Wl,-Map,"86_Stepper_motor_drive.map" --specs=nano.specs -u _printf_float --specs=nosys.specs -Wl,--print-memory-usage -o "86_Stepper_motor_drive.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@	@
86_Stepper_motor_drive.hex: 86_Stepper_motor_drive.elf
	@	riscv-none-embed-objcopy -O ihex "86_Stepper_motor_drive.elf"  "86_Stepper_motor_drive.hex"
	@	@
86_Stepper_motor_drive.lst: 86_Stepper_motor_drive.elf
	@	riscv-none-embed-objdump --source --all-headers --demangle --line-numbers --wide "86_Stepper_motor_drive.elf" > "86_Stepper_motor_drive.lst"
	@	@
86_Stepper_motor_drive.siz: 86_Stepper_motor_drive.elf
	@	riscv-none-embed-size --format=berkeley "86_Stepper_motor_drive.elf"
	@	@
# 其他目标
